{
  "name": "shopping-cart",
  "version": "0.0.1",
  "description": "Shopping cart",
  "main": "build/index.js",
  "keywords": [],
  "dependencies": {
    "mariadb": "^3.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@stryker-mutator/typescript-checker": "^9.3.0",
    "@testcontainers/mariadb": "^11.11.0",
    "@types/jest": "^28.1.6",
    "@types/jest-when": "^3.5.5",
    "archunit": "^2.1.63",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3",
    "jest-when": "^3.7.0",
    "testcontainers": "^11.11.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "compile": "tsc --project tsconfig.json",
    "typecheck:test": "tsc --project tsconfig.test.json --noEmit",
    "test": "npm run typecheck:test && npm run test:unit && npm run test:integration",
    "test:unit": "jest --selectProjects unit",
    "test:fast": "npm run test:unit",
    "test:integration": "jest --selectProjects integration --runInBand",
    "test:unit_watch": "jest --selectProjects unit --watch",
    "test:coverage": "npm run typecheck:test && jest --selectProjects all --coverage",
    "test:mutants": "stryker run",
    "test:fast-mutants": "stryker run stryker-fast.conf.json",
    "test:persistence-mutants": "stryker run stryker-persistence.conf.json",
    "test:persistence": "jest --selectProjects integration --runInBand tests/infrastructure/integration/MariaDBDiscountsRepository.spec.ts tests/infrastructure/integration/MariaDBProductsRepository.spec.ts"
  }
}
